    "es6-promise": "^4.2.6",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.5.1",
    "jquery": "^3.3.1",
    "laravel-mix": "^4.0.14",
    "motion-ui": "^2.0.3",
    "slick-carousel": "^1.8.1",
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "resolve-url-loader": "^2.3.1",
